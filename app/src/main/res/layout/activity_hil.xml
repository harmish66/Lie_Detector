<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/main_bg"
    tools:context="first.hp.liedetector.HilActivity">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/titale"
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:layout_marginBottom="5dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/back"
            android:layout_width="40dp"
            android:layout_height="40dp"
            android:layout_marginStart="10dp"
            android:src="@drawable/baseline_arrow_back_ios_24"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <TextView
            android:id="@+id/name"
            android:layout_width="wrap_content"
            android:layout_height="40dp"
            android:text="Hilaraious Sound"
            android:textColor="@color/white"
            android:textSize="25dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/air"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_constraintEnd_toStartOf="@id/laugh"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/titale">

        <ImageView
            android:id="@+id/ic_bg"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/ic_bg"
            app:layout_constraintBottom_toTopOf="@id/text_bg"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ic_air_horn"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/ic_cat"
            android:tint="@color/pink"
            app:layout_constraintBottom_toBottomOf="@id/ic_bg"
            app:layout_constraintEnd_toEndOf="@id/ic_bg"
            app:layout_constraintStart_toStartOf="@id/ic_bg"
            app:layout_constraintTop_toTopOf="@id/ic_bg" />


        <ImageView
            android:id="@+id/text_bg"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:src="@drawable/txt_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ic_bg" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cat"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/text_bg"
            app:layout_constraintEnd_toEndOf="@id/text_bg"
            app:layout_constraintStart_toStartOf="@id/text_bg"
            app:layout_constraintTop_toTopOf="@id/text_bg" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/laugh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_constraintEnd_toStartOf="@id/claps"
        app:layout_constraintStart_toEndOf="@id/air"
        app:layout_constraintTop_toBottomOf="@id/titale">

        <ImageView
            android:id="@+id/ic_bg2"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/ic_bg"
            app:layout_constraintBottom_toTopOf="@id/text_bg2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ic_laughing"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/ic_broke"
            android:tint="@color/green"
            app:layout_constraintBottom_toBottomOf="@id/ic_bg2"
            app:layout_constraintEnd_toEndOf="@id/ic_bg2"
            app:layout_constraintStart_toStartOf="@id/ic_bg2"
            app:layout_constraintTop_toTopOf="@id/ic_bg2" />


        <ImageView
            android:id="@+id/text_bg2"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:src="@drawable/txt_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ic_bg2" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Broke"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/text_bg2"
            app:layout_constraintEnd_toEndOf="@id/text_bg2"
            app:layout_constraintStart_toStartOf="@id/text_bg2"
            app:layout_constraintTop_toTopOf="@id/text_bg2" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/claps"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/laugh"
        app:layout_constraintTop_toBottomOf="@id/titale">

        <ImageView
            android:id="@+id/ic_bg3"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/ic_bg"
            app:layout_constraintBottom_toTopOf="@id/text_bg3"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ic_clapps"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/ic_mouse"
            android:tint="#FF5722"
            app:layout_constraintBottom_toBottomOf="@id/ic_bg3"
            app:layout_constraintEnd_toEndOf="@id/ic_bg3"
            app:layout_constraintStart_toStartOf="@id/ic_bg3"
            app:layout_constraintTop_toTopOf="@id/ic_bg3" />


        <ImageView
            android:id="@+id/text_bg3"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:src="@drawable/txt_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ic_bg3" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Mouse"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/text_bg3"
            app:layout_constraintEnd_toEndOf="@id/text_bg3"
            app:layout_constraintStart_toStartOf="@id/text_bg3"
            app:layout_constraintTop_toTopOf="@id/text_bg3" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/fart"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_constraintEnd_toStartOf="@id/whistle"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/air">

        <ImageView
            android:id="@+id/ic_bg4"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/ic_bg"
            app:layout_constraintBottom_toTopOf="@id/text_bg4"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ic_fart"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/ic_dog"
            android:tint="#2196F3"
            app:layout_constraintBottom_toBottomOf="@id/ic_bg4"
            app:layout_constraintEnd_toEndOf="@id/ic_bg4"
            app:layout_constraintStart_toStartOf="@id/ic_bg4"
            app:layout_constraintTop_toTopOf="@id/ic_bg4" />


        <ImageView
            android:id="@+id/text_bg4"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:src="@drawable/txt_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ic_bg4" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Dog"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/text_bg4"
            app:layout_constraintEnd_toEndOf="@id/text_bg4"
            app:layout_constraintStart_toStartOf="@id/text_bg4"
            app:layout_constraintTop_toTopOf="@id/text_bg4" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/whistle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_constraintEnd_toStartOf="@id/punch"
        app:layout_constraintStart_toEndOf="@id/fart"
        app:layout_constraintTop_toBottomOf="@id/laugh">

        <ImageView
            android:id="@+id/ic_bg5"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/ic_bg"
            app:layout_constraintBottom_toTopOf="@id/text_bg5"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ic_whistle"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/ic_bomb"
            android:tint="@color/red"
            app:layout_constraintBottom_toBottomOf="@id/ic_bg5"
            app:layout_constraintEnd_toEndOf="@id/ic_bg5"
            app:layout_constraintStart_toStartOf="@id/ic_bg5"
            app:layout_constraintTop_toTopOf="@id/ic_bg5" />


        <ImageView
            android:id="@+id/text_bg5"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:src="@drawable/txt_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ic_bg5" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Bomb"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/text_bg5"
            app:layout_constraintEnd_toEndOf="@id/text_bg5"
            app:layout_constraintStart_toStartOf="@id/text_bg5"
            app:layout_constraintTop_toTopOf="@id/text_bg5" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/punch"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/whistle"
        app:layout_constraintTop_toBottomOf="@id/claps">

        <ImageView
            android:id="@+id/ic_bg6"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/ic_bg"
            app:layout_constraintBottom_toTopOf="@id/text_bg6"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ic_punch"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/ic_fart"
            android:tint="#FFEB3B"
            app:layout_constraintBottom_toBottomOf="@id/ic_bg6"
            app:layout_constraintEnd_toEndOf="@id/ic_bg6"
            app:layout_constraintStart_toStartOf="@id/ic_bg6"
            app:layout_constraintTop_toTopOf="@id/ic_bg6" />


        <ImageView
            android:id="@+id/text_bg6"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:src="@drawable/txt_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ic_bg6" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Fart"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/text_bg6"
            app:layout_constraintEnd_toEndOf="@id/text_bg6"
            app:layout_constraintStart_toStartOf="@id/text_bg6"
            app:layout_constraintTop_toTopOf="@id/text_bg6" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/omg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_constraintEnd_toStartOf="@id/police"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fart">

        <ImageView
            android:id="@+id/ic_bg7"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/ic_bg"
            app:layout_constraintBottom_toTopOf="@id/text_bg7"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ic_omg"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/ic_door"
            android:tint="#673AB7"
            app:layout_constraintBottom_toBottomOf="@id/ic_bg7"
            app:layout_constraintEnd_toEndOf="@id/ic_bg7"
            app:layout_constraintStart_toStartOf="@id/ic_bg7"
            app:layout_constraintTop_toTopOf="@id/ic_bg7" />


        <ImageView
            android:id="@+id/text_bg7"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:src="@drawable/txt_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ic_bg7" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Door"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/text_bg7"
            app:layout_constraintEnd_toEndOf="@id/text_bg7"
            app:layout_constraintStart_toStartOf="@id/text_bg7"
            app:layout_constraintTop_toTopOf="@id/text_bg7" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/police"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_margin="20dp"
        app:layout_constraintEnd_toStartOf="@id/remote"
        app:layout_constraintStart_toEndOf="@id/omg"
        app:layout_constraintTop_toBottomOf="@id/whistle">

        <ImageView
            android:id="@+id/ic_bg8"
            android:layout_width="55dp"
            android:layout_height="55dp"
            android:background="@drawable/ic_bg"
            app:layout_constraintBottom_toTopOf="@id/text_bg8"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/ic_police"
            android:layout_width="28dp"
            android:layout_height="28dp"
            android:src="@drawable/ic_ghost"
            android:tint="#009688"
            app:layout_constraintBottom_toBottomOf="@id/ic_bg8"
            app:layout_constraintEnd_toEndOf="@id/ic_bg8"
            app:layout_constraintStart_toStartOf="@id/ic_bg8"
            app:layout_constraintTop_toTopOf="@id/ic_bg8" />


        <ImageView
            android:id="@+id/text_bg8"
            android:layout_width="90dp"
            android:layout_height="30dp"
            android:src="@drawable/txt_bg"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/ic_bg8" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Ghost"
            android:textColor="@color/white"
            app:layout_constraintBottom_toBottomOf="@id/text_bg8"
            app:layout_constraintEnd_toEndOf="@id/text_bg8"
            app:layout_constraintStart_toStartOf="@id/text_bg8"
            app:layout_constraintTop_toTopOf="@id/text_bg8" />
    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.constraintlayout.widget.ConstraintLayout>